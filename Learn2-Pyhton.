        "#tipe data Bolean\n",
        "print (True)\n",
        "\n",
        "#tipe data String\n",
        "print(\"string dengan menggunakan tenda kutip dua\")\n",
        "print('ini string menggunakan tanda kutip satu')\n",
        "#tipe data string dengan menjelaskan spesial karakter atau escape squances\n",
        "print('ini adalah tanda single quote(\\')')\n",
        "print(\"ini adalah tanda double quote(\\\")\")\n",
        "print(\"ini adalah tanda slash(\\\\)\")\n",
        "print(\"Algoritma\\nPemrograman\") #mengunakan\\n\n",
        "print(\"Algoritma\\bPemrograman\") #menggunakan\\b\n",
        "print(\"Algoritma\\tPemrograman\") #menggunakan\\t\n",
        "print(\"Algoritma\\rPemrobraman\") #menggunakan\\b\n",
        "\n",
        "#tipe data integer\n",
        "print(20)\n",
        "\n",
        "#tipe data Float\n",
        "print(3.14)\n",
